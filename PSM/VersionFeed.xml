  <entry>
    <id>urn:uuid:2EFC7337-C652-4C09-92F8-68EDFADA4C0C</id>
    <title>1.0.9 (1.0.9.2)</title>
    <summary>1.0.9.2</summary>
    <link href="http://servermanagers.freeforums.net/thread/9/downloads" />
    <updated>2018-05-27T03:30:00Z</updated>
